{
   "background": {
      "persistent": false,
      "scripts": [ "js/chrome/background.js", "js/libs/algoliasearch.js", "js/chrome/omnibox.js" ]
   },
   "content_scripts": [ {
      "css": [ "css/content.css" ],
      "js": [ "js/libs/jquery-3.1.1.min.js", "js/libs/hogan-3.0.1.js", "js/libs/typeahead.bundle.js", "js/libs/algoliasearch.js", "js/content.js" ],
      "matches": [ "https://github.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "Add instant search capabilities to GitHub's search bar.",
   "icons": {
      "128": "images/icon.png",
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "64": "images/icon-64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Awesome Autocomplete for GitHub",
   "omnibox": {
      "keyword": "aa"
   },
   "page_action": {
      "default_icon": "images/icon.png",
      "default_popup": "html/chrome.html",
      "default_title": "Awesome Autocomplete for GitHub"
   },
   "permissions": [ "https://github.com/*", "activeTab", "storage", "declarativeContent", "https://github.algolia.com/*", "*://*.algolia.net/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.7.0",
   "web_accessible_resources": [ "images/*" ]
}
